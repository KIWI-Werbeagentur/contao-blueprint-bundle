{
    "name": "kiwi/contao-blueprints",
    "type": "contao-bundle",
    "keywords": ["contao", "blueprints", "templates", "article templates", "preview"],
    "description": "This Contao bundle allows you to create reusable article blueprints with preview when inserting.",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Katrin Zimmermann",
            "homepage": "https://github.com/diekatrin"
        },
        {
            "name": "KIWI. Werbeagentur GmbH",
            "homepage": "https://www.kiwi.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "contao/core-bundle": "~5.3.13",
        "oneup/contao-backend-sortable-list-views": "^1.1.1"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4" : {
            "Kiwi\\Contao\\BlueprintsBundle\\" : "src/"
        }
    },
    "extra":{
        "contao-manager-plugin": "Kiwi\\Contao\\BlueprintsBundle\\ContaoManager\\Plugin"
    }
}

