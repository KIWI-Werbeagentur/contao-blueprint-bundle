{
    "name": "kiwi/contao-blueprints",
    "type": "contao-bundle",
    "description": "This contao-bundle is intended to enhance the editors UX of your contao installation. It allows you to create reusable article blueprints so that editor doesn't have to build them from scratch.",
    "license": "proprietary",
    "authors": [
        {
            "name": "Katrin Zimmermann",
            "homepage": "https://github.com/diekatrin"
        },
        {
            "name": "KIWI. Werbeagentur GmbH",
            "homepage": "https://www.kiwi.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "contao/core-bundle": "~5.3.13",
        "oneup/contao-backend-sortable-list-views": "^1.1.1"
    },
    "conflict": {
        "contao/manager-plugin": "<2.0 || >=3.0"
    },
    "autoload": {
        "psr-4" : {
            "Kiwi\\Contao\\Blueprints\\" : "src/"
        }
    },
    "extra":{
        "contao-manager-plugin": "Kiwi\\Contao\\Blueprints\\ContaoManager\\Plugin"
    }
}
